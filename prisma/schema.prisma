generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Ticket {
  id        String   @id @default(cuid())
  title     String
  content   String
  status    Status   @default(OPEN)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Status {
  OPEN
  IN_PROGRESS
  DONE
}
